{
  "name": "artistats",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "build": "npm run write:env -s && ng build",
    "start": "npm run write:env -s && ng serve -o --ssl true --ssl-key ./certificates/localhost-key.pem --ssl-cert ./certificates/localhost.pem",
    "serve:sw": "npm run build -s && npx http-server ./dist -p 4200 -o --ssl true --ssl-key ./certificates/localhost-key.pem --ssl-cert ./certificates/localhost.pem",
    "lint": "ng lint && stylelint \"src/**/*.scss\"",
    "test": "npm run write:env -s && ng test",
    "test:ci": "npm run write:env -s && npm run lint -s && ng test --configuration=ci",
    "e2e": "npm run write:env -s && ng e2e",
    "cypress:open": "npm run write:env -s && ng run artistats:cypress-open",
    "cypress:run": "npm run write:env -s && ng run artistats:cypress-run",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean --sort",
    "write:env": "ngx-scripts env npm_package_version",
    "prettier": "prettier --write \"./src/**/*.{ts,js,html,scss}\"",
    "prettier:check": "prettier --list-different \"./src/**/*.{ts,js,html,scss}\"",
    "postinstall": "npm run prettier -s && husky install",
    "generate": "ng generate"
  },
  "dependencies": {
    "@angular/animations": "~14.1.3",
    "@angular/cdk": "~14.1.3",
    "@angular/common": "~14.1.3",
    "@angular/compiler": "~14.1.3",
    "@angular/core": "~14.1.3",
    "@angular/flex-layout": "^14.0.0-beta.40",
    "@angular/forms": "~14.1.3",
    "@angular/localize": "~14.1.3",
    "@angular/material": "~14.1.3",
    "@angular/platform-browser": "~14.1.3",
    "@angular/platform-browser-dynamic": "~14.1.3",
    "@angular/router": "~14.1.3",
    "@angular/service-worker": "~14.1.3",
    "@ngx-translate/core": "^14.0.0",
    "angular-spotify": "^1.5.1",
    "angulartics2": "^12.1.0",
    "axios": "^0.27.2",
    "material-design-icons-iconfont": "^6.7.0",
    "moment": "^2.29.4",
    "rxjs": "^7.5.6",
    "spotify-web-api-js": "^1.5.2",
    "tslib": "^2.4.0",
    "zone.js": "~0.11.8"
  },
  "devDependencies": {
    "@angular-builders/jest": "^14.0.1",
    "@angular-devkit/build-angular": "~14.1.3",
    "@angular-eslint/builder": "~14.0.3",
    "@angular-eslint/eslint-plugin": "~14.0.3",
    "@angular-eslint/eslint-plugin-template": "~14.0.3",
    "@angular-eslint/schematics": "~14.0.3",
    "@angular-eslint/template-parser": "~14.0.3",
    "@angular/cli": "~14.1.3",
    "@angular/compiler-cli": "~14.1.3",
    "@angular/language-service": "~14.1.3",
    "@biesbjerg/ngx-translate-extract": "^7.0.4",
    "@biesbjerg/ngx-translate-extract-marker": "^1.0.0",
    "@cypress/schematic": "^2.0.3",
    "@ngneat/until-destroy": "^9.2.2",
    "@ngx-rocket/scripts": "^5.2.2",
    "@types/jest": "^28.1.8",
    "@types/node": "^18.7.13",
    "@typescript-eslint/eslint-plugin": "~5.35.1",
    "@typescript-eslint/parser": "~5.35.1",
    "cypress": "~10.6.0",
    "eslint": "^8.22.0",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsdoc": "latest",
    "eslint-plugin-prefer-arrow": "latest",
    "htmlhint": "^1.1.4",
    "https-proxy-agent": "^5.0.1",
    "husky": "^8.0.1",
    "jest": "^29.0.0",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "stylelint": "~14.11.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-scss": "~7.0.0",
    "stylelint-config-standard": "~28.0.0",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "~4.7.4"
  },
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.scss",
        "options": {
          "singleQuote": false
        }
      }
    ]
  },
  "engines": {
    "npm": "please-use-yarn",
    "yarn": ">= 1.19.1"
  }
}
